group("flutter") {
  testonly = true

  deps = [ "//flutter/sky" ]

  if (is_fuchsia) {
    deps += [
      "//flutter/content_handler",
      "//flutter/examples",
      "//flutter/flow",
      "//flutter/runtime",
      "//flutter/snapshotter",
      "//flutter/snapshotter($host_toolchain)",
    ]
  }
}

if (!is_fuchsia) {

  group("dist") {
    testonly = true

    deps = [
      "//flutter/sky/dist",
    ]
  }

}
